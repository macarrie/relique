port            = 8433
bind_addr       = "0.0.0.0"
public_address  = "localhost"

ssl_cert    = "__ROOT__/etc/relique/certs/cert.pem"
ssl_key     = "__ROOT__/etc/relique/certs/key.pem"
strict_ssl_certificate_check = true

client_cfg_path     = "clients"
schedules_cfg_path  = "schedules"
backup_storage_path = "/opt/relique"
module_install_path = "__ROOT__/var/lib/relique/modules"
retention_path      = "__ROOT__/var/lib/relique/retention.dat"
db_path				= "__ROOT__/var/lib/relique/db/server.db"
